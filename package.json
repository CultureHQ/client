  "description": "A JavaScript client that wraps the CultureHQ API",
  "main": "dist/client.js",
  "module": "src/client.js",
  "scripts": {
    "build": "rm -r dist && babel src --out-dir dist --ignore '**/__tests__' && cp src/calls.json dist/calls.json",
    "lint": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:CultureHQ/client.git"
  },
  "author": "Kevin Deisz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CultureHQ/client/issues"
  },
  "homepage": "https://github.com/CultureHQ/client#readme",
  "dependencies": {
    "actioncable": "^5.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.2.3",
    "@culturehq/eslint-config": "^0.2.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^24.1.0",
    "eslint": "^5.14.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-form-data": "^2.0.0",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "url-polyfill": "^1.1.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
